{
    "manifest_version": 2,

    "name": "CommandPlus",
    "version": "1.0.20",

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },

    "browser_action": {
        "default_title": "CommandPlus",
        "default_popup": "browser-action/popup.html"
    },

    "content_scripts": [{
        "matches": ["https://en.wikipedia.org/*"],
        "js": ["jquery/jquery.min.js", "content-script.js"]
    }],

    "permissions": ["tabs", "<all_urls>"]
}
